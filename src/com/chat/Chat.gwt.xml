<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="chat">

	<inherits name='com.google.gwt.user.User' />
    <inherits name="com.google.gwt.rpc.RPC" />

    <replace-with class="com.chat.client.ClientFactoryImpl">
       <when-type-is class="com.chat.client.ClientFactory"/>
    </replace-with>

	<entry-point class='com.chat.client.Chat' />
	<source path='client' />
    <source path='shared'/>
    <public path="public" />

    <stylesheet src="normalize.css"/>
    <stylesheet src="chat.css"/>
</module>
